{
  "expo": {
    "name": "DoctorAPP",
    "slug": "DoctorAPP",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/image/bkh1.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image":"./assets/image/bkhcare1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "versionCode":1,
      "package": "com.doctor_app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["@config-plugins/react-native-webrtc"]
  }
}
